---
import LetsTalk from "~/components/inputs/LetsTalk.astro";
import Header, { HeaderVariant } from "~/components/nav/Header.astro";
import Layout from "~/layouts/Layout.astro";

import * as data from "~/content/privacy.mdx";
import { Content } from "~/content/privacy.mdx";
---

<Layout title={data.frontmatter.title}>
  {
    data.frontmatter.header && (
      <Header
        variant={HeaderVariant.Fixed}
        items={data.frontmatter.header.ref}
      />
    )
  }

  <Content />
  <LetsTalk />
</Layout>

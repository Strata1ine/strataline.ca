---
import ServiceGallery from "~/components/nav/ServiceGallery.astro";

const { title, titlePos, ...slot } = Astro.props;
---

<ServiceGallery
  title={title}
  exclude={Astro.locals.index.data.popular}
  titlePos={titlePos}
  {...slot}
>
  <slot name="top" slot="top" />
  <slot name="bottom" slot="bottom" />
</ServiceGallery>
